 <div class="{{if sidebarOpen "toggled"}}"  id="wrapper">

    {{main-sidebar
      model=model
      updateCurrentSection=(action 'updateCurrentList')
      currentSection=currentList
    }}

    <div id="page-content-wrapper">
      <div class="container-fluid">

        <button class="sidebar__toggle" {{action 'toggleSidebar'}}>
          {{#if sidebarOpen}}
            {{fa-icon "angle-double-left"
              class="fa"
            }}
          {{else}}
            {{fa-icon "angle-double-right"
              class="fa"
            }}
          {{/if}}
        </button>

        <!-- child route template (outlet)  -->
        <div class="row">
          <div class="col-lg-12">
          {{checklist-list checklist=currentListData}}
          </div>
        </div>
        <!-- #/child route template  -->

      </div>
    </div>
    <!-- /#page-content-wrapper  -->

  </div>